datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User{
    id          Int         @id @default(autoincrement())
    email       String      @unique
    name        String
    password    String
    createdAt   DateTime    @default(now())
    attAt       DateTime    @updatedAt
}